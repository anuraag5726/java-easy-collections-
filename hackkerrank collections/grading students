 List<Integer> li = new ArrayList<>();
        for(int i=0;i<grades.size();i++){
            if(5-grades.get(i)%5<3 && grades.get(i)>=38){
                li.add(grades.get(i)+(5-grades.get(i)%5));
            }
            else if(5-grades.get(i)==3 && grades.get(i)>=38){
                li.add(grades.get(i));
            }
            else{
                li.add(grades.get(i));
            }
        }
        return li;
